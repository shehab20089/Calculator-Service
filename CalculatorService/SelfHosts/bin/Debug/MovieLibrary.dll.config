<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <appSettings>
    <add key="aspnet:UseTaskFriendSynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6.1" />
  </system.web>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="metadata" name="CalculatorServiceLibrary.CalculatorService">
        <endpoint address="http://localhost:8888/user" binding="basicHttpBinding" contract="CalculatorServiceLibrary.ICalculatorService"></endpoint>
        <endpoint bindingConfiguration="tcpnotsecure" address="net.tcp://localhost:8658/user" binding="netTcpBinding" contract="CalculatorServiceLibrary.ICalculatorService"></endpoint>
        <endpoint address="net.pipe://localhost/user" binding="netNamedPipeBinding" contract="CalculatorServiceLibrary.ICalculatorService"></endpoint>
        <endpoint bindingConfiguration="tcptextsecureless" address="net.tcp://localhost:9457/user" binding="customBinding" contract="CalculatorServiceLibrary.ICalculatorService"></endpoint>
        <endpoint address="http://localhost:8888/user/mex" binding="mexHttpBinding" contract="IMetadataExchange"></endpoint>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="metadata">
          <serviceMetadata httpGetEnabled="true" httpGetUrl="http://localhost:8888/user/meta" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <bindings>
      <customBinding>
        <binding name="tcptextsecureless">
          <textMessageEncoding></textMessageEncoding>
          <tcpTransport></tcpTransport>
        </binding>
      </customBinding>
      <netTcpBinding>
        <binding name="tcpnotsecure">
          <security mode="None"></security>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" />
    <directoryBrowse enabled="true" />
  </system.webServer>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <connectionStrings>
    <add name="UsersCS" connectionString="Data Source=.;Initial Catalog=UsersDb ;Integrated Security=True; " providerName="System.Data.SqlClient"/>

  </connectionStrings>
</configuration>